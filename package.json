{
  "name": "website-mobile",
  "version": "1.0.0",
  "description": "Dreamlines website mobile frontend probject",
  "scripts": {
    "webpack:watch": "webpack-dev-server --devtool eval --progress --content-base src",
    "server:watch": "nodemon --exec ts-node -- ./src/server.tsx",
    "watch": "npm run server:watch & npm run webpack:watch",
    "build:prod:frontend": "NODE_ENV=production rm -rf assets/dist && webpack --config webpack.config.prod.js",
    "build:prod:backend": "rm -rf dist && tsc -p .",
    "lint": "tslint  -c tslint.json 'src/**/**.ts' 'src/**/**.tsx'",
    "build:prod": "npm run build:prod:frontend && npm run build:prod:backend",
    "test:jest": "jest --config jest.config.json",
    "test": "npm run lint && npm run test:jest",
    "start": "NODE_ENV=production node ./dist/server.js"
  },
